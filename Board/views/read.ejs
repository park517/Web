                <div class="input_type">  
                    <label > 내용</label>
                    <textarea readonly name="content" id="content" cols="70" rows="20"><%=list[0].context%></textarea>
                </div>
                <div class="btn">
                    <!-- onClick="location.href='/read/update/<%=list[0].number%>'"  -->
                    <button class ='<%=list[0].number%>' id ="update" name="123" >수정</button>
                    <button id ="delete" >삭제</button>

                </div>
               
            </div>


    

      <script src="/js/read.js"></script>  
      
    </body>
</html>